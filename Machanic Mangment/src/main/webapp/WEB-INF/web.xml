<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Machanic Mangment</display-name>
  
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>machanic_Login.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	 <servlet>
		<servlet-name>Machanic_insert </servlet-name>
		<servlet-class>machanic_Login.Machanic_InsertServlet </servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Machanic_insert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
   
	<servlet>
		<servlet-name>updateMachanic</servlet-name>
		<servlet-class>machanic_Login.Machanic_UpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateMachanic</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteMachanic</servlet-name>
		<servlet-class>machanic_Login.Machanic_DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteMachanic</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
</web-app>