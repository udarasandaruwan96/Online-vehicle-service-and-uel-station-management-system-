<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.Supplier.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	 <servlet>
		<servlet-name>supplierinsert </servlet-name>
		<servlet-class>com.Supplier.SupplierInsertServlet </servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>supplierinsert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
   
	<servlet>
		<servlet-name>updatesupplier</servlet-name>
		<servlet-class>com.Supplier.SupplierUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updatesupplier</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deletesupplier</servlet-name>
		<servlet-class>com.Supplier.DeleteSupplierServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deletesupplier</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
</web-app>